{
  "name": "surf-wallet",
  "description": "A Surf wallet browser extension",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build && mv .output/chrome-mv3 .output/surf-wallet",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@webext-core/messaging": "^2.2.0",
    "bip39": "^3.1.0",
    "crypto-js": "^4.2.0",
    "ethers": "^6.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^4.12.0",
    "@turnkey/api-key-stamper": "0.4.6",
    "@turnkey/http": "3.4.1",
    "@turnkey/iframe-stamper": "2.5.0",
    "@turnkey/sdk-browser": "5.2.0",
    "@turnkey/ethers": "1.1.28"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@wxt-dev/module-react": "^1.1.3",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.8.3",
    "util": "^0.12.5",
    "wxt": "^0.20.6"
  }
}
